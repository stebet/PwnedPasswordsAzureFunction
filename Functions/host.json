{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "",
      "customHeaders": {
        "Content-Type":  "text/plain",
        "Cache-Control": "public; max-age=2678400",
        "Arr-Disable-Session-Affinity": "True",
        "Access-Control-Allow-Origin": "*"
      }
    }
  },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "EnableDependencyTracking": true,
      "samplingSettings": {
        "isEnabled": false
      }
    }
  }
}